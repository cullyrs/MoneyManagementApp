{
  "name": "moneymanagementapp",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "test": "jest",
    "test:log": "jest --json --outputFile=test-results.json",
    "test:xml": "jest --reporters=default --reporters=jest-junit",
    "test:html": "jest --reporters=default --reporters=jest-html-reporter"
  },
  "jest": {
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "outputDirectory": "./test-results"
        }
      ],
      [
        "jest-html-reporter",
        {
          "outputPath": "./test-results/test-report.html"
        }
      ]
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "electron": "^33.3.1",
    "jest": "^29.7.0",
    "jest-html-reporter": "^3.10.2",
    "jest-junit": "^16.0.0",
    "jsdom": "^26.0.0"
  },
  "dependencies": {
    "moneymanagementapp": "file:",
    "mongodb": "^6.13.0",
    "mongoose-double": "^0.0.1",
    "mongoose-int32": "^0.6.0",
    "node-fetch": "^3.3.2"
  }
}
